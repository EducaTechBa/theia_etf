    "@theia/output": "1.6.0",
    "@theia/plugin-dev": "1.6.0",
    "@theia/plugin-ext": "1.6.0",
    "@theia/plugin-ext-vscode": "1.6.0",
    "@theia/preferences": "1.6.0",
    "@theia/preview": "1.6.0",
    "@theia/process": "1.6.0",
    "@theia/search-in-workspace": "1.6.0",
    "@theia/task": "1.6.0",
    "@theia/userstorage": "1.6.0",
