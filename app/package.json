    "react-select": "^3.1.0",
    "top-bar": "0.0.0",
    "uup-game-view": "0.0.0"
  },
  "devDependencies": {
    "@theia/cli": "latest"
  },
  "scripts": {
    "prepare": "yarn run clean && yarn build && yarn run download:plugins",
    "prepare-dev": "yarn run clean && yarn build-dev && yarn run download:plugins",
    "clean": "theia clean",
    "build": "theia build --mode production",
    "build-dev": "theia build --mode development",
    "download:plugins": "theia download:plugins",
    "start": "theia start --plugins=local-dir:plugins",
    "watch": "theia build --mode development"
  },
  "theia": {
    "target": "browser",
    "frontend": {
      "config": {
        "applicationName": "Theia ETF"
      }
    }
  },
  "theiaPluginsDir": "plugins",
  "theiaPlugins": {
    "vscode-cpptools": "https://github.com/microsoft/vscode-cpptools/releases/download/0.29.0/cpptools-linux.vsix",
    "vscode-cpp": "https://open-vsx.org/api/vscode/cpp/1.49.2/file/vscode.cpp-1.49.2.vsix",
    "vscode-clangd": "https://open-vsx.org/api/llvm-vs-code-extensions/vscode-clangd/0.1.7/file/llvm-vs-code-extensions.vscode-clangd-0.1.7.vsix",
    "vscode-builtin-java": "https://github.com/theia-ide/vscode-builtin-extensions/releases/download/v1.39.1-prel/java-1.39.1-prel.vsi>
    "vscode-builtin-json": "https://github.com/theia-ide/vscode-builtin-extensions/releases/download/v1.39.1-prel/json-1.39.1-prel.vsi>
    "vscode-builtin-configuration-editing": "https://github.com/theia-ide/vscode-builtin-extensions/releases/download/v1.39.1-prel/con>  }
