{
  "private": true,
  "name": "app",
  "version": "0.0.0",
  "dependencies": {
    "@theia/core": "1.66.2",
    "@theia/debug": "1.66.2",
    "@theia/editor": "1.66.2",
    "@theia/file-search": "1.66.2",
    "@theia/filesystem": "1.66.2",
    "@theia/keymaps": "1.66.2",
    "@theia/markers": "1.66.2",
    "@theia/messages": "1.66.2",
    "@theia/mini-browser": "1.66.2",
    "@theia/monaco": "1.66.2",
    "@theia/navigator": "1.66.2",
    "@theia/output": "1.66.2",
    "@theia/plugin-dev": "1.66.2",
    "@theia/plugin-ext": "1.66.2",
    "@theia/plugin-ext-vscode": "1.66.2",
    "@theia/preferences": "1.66.2",
    "@theia/preview": "1.66.2",
    "@theia/process": "1.66.2",
    "@theia/search-in-workspace": "1.66.2",
    "@theia/task": "1.66.2",
    "@theia/userstorage": "1.66.2",
    "@theia/variable-resolver": "1.66.2",
    "@theia/vsx-registry": "1.66.2",
    "@theia/workspace": "1.66.2",
    "ai-asistent": "0.0.0",
    "assignments-view": "0.0.0",
    "autotest-view": "0.0.0",
    "react-select": "^3.1.0",
    "top-bar": "0.0.0",
    "uup-game-view": "0.0.0"
  },
  "devDependencies": {
    "@theia/cli": "latest"
  },
  "scripts": {
    "prepare": "yarn run clean && yarn build && yarn run download:plugins",
    "prepare-dev": "yarn run clean && yarn build-dev && yarn run download:plugins",
    "clean": "theia clean",
    "build": "theia build --mode production",
    "build-dev": "theia build --mode development",
    "download:plugins": "theia download:plugins",
    "start": "theia start --plugins=local-dir:plugins",
    "watch": "theia build --mode development"
  },
  "theia": {
    "target": "browser",
    "frontend": {
      "config": {
        "applicationName": "EducaTech Theia",
        "warnOnPotentiallyInsecureHostPattern": false
      }
    },
    "backend": {
      "config": {
        "miniBrowserEndpoint": {
          "path": "/mini-browser"
        },
        "warnOnPotentiallyInsecureHostPattern": false
      }
    }
  },
  "theiaPluginsDir": "plugins",
  "theiaPlugins": {
    "vscode-git": "https://open-vsx.org/api/vscode/git/1.52.1/file/vscode.git-1.52.1.vsix",
    "vscode-clangd": "https://open-vsx.org/api/llvm-vs-code-extensions/vscode-clangd/0.3.2/file/llvm-vs-code-extensions.vscode-clangd-0.3.2.vsix",
    "vscode-lldb": "https://github.com/vadimcn/codelldb/releases/download/v1.11.8/codelldb-linux-arm64.vsix",
    "vscode-builtin-cpp": "https://open-vsx.org/api/vscode/cpp/1.52.1/file/vscode.cpp-1.52.1.vsix",
    "vscode-builtin-python": "https://open-vsx.org/api/vscode/python/1.52.1/file/vscode.python-1.52.1.vsix",
    "vscode-builtin-java": "https://open-vsx.org/api/vscode/java/1.52.1/file/vscode.java-1.52.1.vsix",
    "vscode-builtin-javascript": "https://open-vsx.org/api/vscode/javascript/1.52.1/file/vscode.javascript-1.52.1.vsix",
    "vscode-builtin-php": "https://open-vsx.org/api/vscode/php/1.52.1/file/vscode.php-1.52.1.vsix",
    "vscode-builtin-html": "https://open-vsx.org/api/vscode/html/1.52.1/file/vscode.html-1.52.1.vsix",
    "vscode-builtin-css": "https://open-vsx.org/api/vscode/css/1.52.1/file/vscode.css-1.52.1.vsix",
    "vscode-builtin-json": "https://open-vsx.org/api/vscode/json/1.52.1/file/vscode.json-1.52.1.vsix"
  }
}
