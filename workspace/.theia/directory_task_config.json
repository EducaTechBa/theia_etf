{
    "task": {
        "run": "/usr/local/webide/runners/universal_runner.sh ${file} ${fileDirname}" 
    },
    "children": {
        "UUP": {
            "task": {
                "run": "gcc ${file} -o ${fileDirname}/.${fileBasenameNoExtension} && ./.${fileBasenameNoExtension}"
            }
        }
    }
}
